{
  "name": "angular-skeleton",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "npm run lint && ng run angular-skeleton:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "lint": "npx eslint \"src/**/*.{js,ts,html}\" --quiet --fix",
    "format": "npx prettier --config ./.prettierrc \"src/**/*.{js,ts,html,scss}\" --write",
    "build:ssr-local": "ng build && ng run angular-skeleton:server",
    "build.prod": "ng build --environment=prod && tar -zcvf archive.tar.gz dist/prod/*",
    "build:ssr-dev": "ng build --configuration development && ng run angular-skeleton:server:development",
    "build:ssr-staging": "ng build --configuration staging && ng run angular-skeleton:server:staging",
    "build:ssr-prod": "ng build --configuration production && ng run angular-skeleton:server:production",
    "prerender:local": "ng run angular-skeleton:prerender:local",
    "prerender:dev": "ng run angular-skeleton:prerender:development",
    "prerender:staging": "ng run angular-skeleton:prerender:staging",
    "prerender:prod": "ng run angular-skeleton:prerender:production",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nguniversal/express-engine": "^13.0.2",
    "check-engine": "^1.10.1",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.15.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "^7.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.6.0",
    "typescript": "~4.5.2"
  },
  "engines": {
    "node": ">=10.0.0 <17.0.0",
    "npm": ">=6.0.0"
  }
}
